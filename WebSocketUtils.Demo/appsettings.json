{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Enrich": [ "FromLogContext", "WithThreadId", "WithMachineName" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/websocket-demo-.log",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] ({ThreadId}) {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "WebSocketDemo": {
    "Redis": {
      "Host": "localhost",
      "Port": 6379,
      "Topic": "demo-topic"
    },
    "Kafka": {
      "BootstrapServers": "localhost:9092",
      "Topic": "demo-topic"
    },
    "ConnectionId": "demo-conn"
  }
}
